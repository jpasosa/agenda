<!DOCTYPE html>
<html> 
        <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
        <title>
            {% block title %} AGENDA  {% endblock %}
        </title>
        
        {% block stylesheets %}
            <!-- <link href='http://fonts.googleapis.com/css?family=Telex' rel='stylesheet' type='text/css'> -->
            <link href="{{ asset('bundles/agendabackend/css/main.css') }}" type="text/css" rel="stylesheet" />
            <link href="{{ asset('bundles/agendauser/css/fosuser.css') }}" type="text/css" rel="stylesheet" />
        {% endblock %}
        

        {% block javascripts %}
            // <script src="http://jqueryjs.googlecode.com/files/jquery-1.2.6.min.js" type="text/javascript"></script>

            <!--[if lt IE 7]>
                <script src="http://ie7-js.googlecode.com/svn/version/2.1(beta4)/IE7.js"></script>
            <![endif]-->

            <!--  jquery core -->
            
            <script src="{{ asset('bundles/agendauser/js/jquery-1.4.1.min.js') }}" type="text/javascript"></script>

            <!-- Custom jquery scripts -->

            <script src="{{ asset('bundles/agendauser/js/custom_jquery.js') }}" type="text/javascript"></script>
            <!-- MUST BE THE LAST SCRIPT IN <HEAD></HEAD></HEAD> png fix -->

            <script src="{{ asset('bundles/agendauser/js/jquery.pngFix.pack.js') }}" type="text/javascript"></script>
        {% endblock %}
    
    </head>
    
    <body>

        <div class="out-container">        
            <div class="header">
                {% block header %}
                    
                {% endblock %}
            </div>

            <div class="content">
                {% block body %}
                    {% block login %} {% endblock login %}

                {% endblock %}
            </div>

            <!-- FOOTER -->
            {# include 'VaxFrontBundle:Page:footer.html.twig' #}
        </div> <!-- cierro out-container -->
    </body>
</html>
